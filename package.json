{
  "name": "food-plaza-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "build": "tsc -p",
    "start:prod": "node ./dist/index.js",
    "typeorm": "typeorm",
    "migration:create": "typeorm migration:create ./src/migrations/$npm_package_config_migrationName",
    "migration:generate": "typeorm migration:generate --dataSource  ./src/data-source.ts  ./src/migrations/updatingTable",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run --dataSource  ./src/data-source.ts --transaction ./src/migrations/CreatingAllTable1711304892325",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert --dataSource ./src/data-source.ts --transaction ./src/migrations/CreatingAllTable1711304892325"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.0.0-beta.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "pg": "^8.11.3",
    "razorpay": "^2.9.2",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-formidable": "^1.2.3",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@types/passport": "^1.0.16",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
